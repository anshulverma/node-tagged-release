#!/usr/bin/env node

(function() {
  var tagr;

  require('coffee-script/register');
  require('../lib/modules/completion');

  process.title = 'tagr';

  tagr = require('../lib/tag-release');

  tagr.cli.start(function(err) {
    if (!err) {
      tagr.cli.log.info('Tagger'.grey + ' done'.green.bold);
    }
    return process.exit(err ? 1 : 0);
  });

}).call(this);
